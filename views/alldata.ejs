<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <title>Document</title>
        <!-- CSS only -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <!-- JavaScript Bundle with Popper -->
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.12.9/dist/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    </head>
<body>
    <%- include('./Navbar.ejs') %>
    <div  class="container">

<h1>All Data gvien below</h1>
<% if(typeof data !== 'undefined' && data){ %> 

    <div class="row jumbotron">
        <div class="col-sm-10 col-lg-10">
            <small>Google Url:<%=data.search_metadata.google_url  %> </small>
            <p>Link :<a href="<%=data.search_metadata.json_endpoint  %>"> <%=data.search_metadata.json_endpoint  %></a></p>
            <p>File :<a href="<%=data.search_metadata.raw_html_file  %>"> <%=data.search_metadata.raw_html_file  %></a></p>
            <small> created_at:<%=data.search_metadata.created_at %>  </small>
            <small> processed_at:<%=data.search_metadata.processed_at %>  </small>
        </div>
    </div>

<hr  />

    <div class="row jumbotron">

        <div class="col-sm-10 col-lg-10">
            <% if(typeof data !== 'undefined' && data.organic_results) {%> 
            <% for(let i  of data.organic_results) {%> 

                <h3><%=i.title %> </h3>
                <p>Link :<a href="<%=i.link  %>"> <%=i.link  %></a></p>
                <p>Display Link :<a href="<%=i.displayed_link  %>"> <%=i.displayed_link  %></a></p>
                <small><%=i.snippet %>  </small>
                <p>About link :<a href="<%=i.about_page_link  %>"> <%=i.about_page_link  %></a></p>
                <% } %> 
         
           
           <% } %> 

            
        </div>
    </div>

    <hr />
    <div class="row jumbotron">
        <div class="col-sm-10 col-lg-10">
            <h5> search_information</h5>
            <p><%=data.search_information.organic_results_state  %> </p>
            <p>Link :<a href="<%=data.search_information.query_displayed  %>"> <%=data.search_information.query_displayed  %></a></p>
            <small>Total  REsult:<%=data.search_information.total_results %>  </small>
        </div>
    </div>

    <hr />
    <div class="row jumbotron">
        <div class="col-sm-10 col-lg-10">
         <h5> related_searches</h5>
            <p>Link :<a href="<%=data.related_searches.link %>"> <%=data.search_information.query_displayed  %></a></p>
           
        </div>
    </div>


<% } %> 
</div>  
</body>
</html>